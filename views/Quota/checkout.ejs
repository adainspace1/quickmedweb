<script>
    // Function to calculate the total
function calculateTotal() {
    // Get all the span tags by their IDs
    const itemsCost = parseFloat(document.getElementById("icost").textContent.replace("$", "")) || 0;
    const subscriptionCost = parseFloat(document.getElementById("subcost").textContent.replace("$", "")) || 0;
    const deliveryCost = parseFloat(document.getElementById("delicost").textContent.replace("$", "")) || 0;
    const vat = parseFloat(document.getElementById("vat").textContent.replace("$", "")) || 0;
    const serviceFees = parseFloat(document.getElementById("serfee").textContent.replace("$", "")) || 0;
    const urgencyFees = parseFloat(document.getElementById("urgfee").textContent.replace("$", "")) || 0;
    const convenienceFees = parseFloat(document.getElementById("convfee").textContent.replace("$", "")) || 0;
    const bookingFees = parseFloat(document.getElementById("boofee").textContent.replace("$", "")) || 0;
    const insuranceFees = parseFloat(document.getElementById("insufee").textContent.replace("$", "")) || 0;

    // Calculate the total
    const total = itemsCost + subscriptionCost + deliveryCost + vat + serviceFees + urgencyFees + convenienceFees + bookingFees + insuranceFees;

    // Display the total in the "ordtotal" span tag
    document.getElementById("ordtotal").textContent = `$${total.toFixed(2)}`;
}

// Call the function to calculate the total
calculateTotal();

</script>

