 <script>
    
        let radio1 = document.getElementById("checked1");
        let radio2 = document.getElementById("checked2");
        let radio3 = document.getElementById("checked3");
        let radio4 = document.getElementById("checked4");
    
     function check (){
    
    
        radio2.checked = false;
        radio3.checked = false;
        radio4.checked = false;
     }
    
     function check2 (){
    
    
    radio1.checked = false;
    radio3.checked = false;
    radio4.checked = false;
    }
    
    function check3 (){
    
    
    radio1.checked = false;
    radio2.checked = false;
    radio4.checked = false;
    }
    
    function check4 (){
    
    
    radio1.checked = false;
    radio2.checked = false;
    radio3.checked = false;
    }
    
    
    document.getElementById("checked1").addEventListener("click", function(){
        document.querySelector(".showplacego").classList.remove("goo2")
        document.querySelector(".showplacego").classList.remove("goo3")
        document.querySelector(".showplacego").classList.remove("goo4")
        check ()
    })
    
    
    document.getElementById("checked2").addEventListener("click", function(){
        document.querySelector(".showplacego").classList.add("goo2")
        document.querySelector(".showplacego").classList.remove("goo3")
        document.querySelector(".showplacego").classList.remove("goo4");
        check2 ()
    })
    
    document.getElementById("checked3").addEventListener("click", function(){
        document.querySelector(".showplacego").classList.add("goo3")
        document.querySelector(".showplacego").classList.remove("goo4")
        check3 ()
    })
    
    document.getElementById("checked4").addEventListener("click", function(){
        document.querySelector(".showplacego").classList.add("goo4")
        check4 ()
    })
    
</script>



